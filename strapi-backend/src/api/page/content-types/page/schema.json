{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": "User-generated landing pages"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "htmlContent": {
      "type": "richtext",
      "required": true
    },
    "pageType": {
      "type": "enumeration",
      "enum": [
        "store",
        "event",
        "serviceProvider",
        "messageInBottle",
        "course",
        "workshop",
        "restaurantMenu",
        "generic"
      ],
      "default": "generic",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "isActive": {
      "type": "boolean",
      "default": false
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "city": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "products": {
      "type": "json",
      "default": []
    },
    "metadata": {
      "type": "json",
      "default": {}
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user.user",
      "inversedBy": "pages"
    },
    "purchases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::purchase.purchase",
      "mappedBy": "page"
    },
    "leads": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lead.lead",
      "mappedBy": "page"
    },
    "analytics": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::analytic.analytic",
      "mappedBy": "page"
    }
  }
}
