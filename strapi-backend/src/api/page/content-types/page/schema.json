{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": "User-generated landing pages"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "htmlContent": {
      "type": "richtext",
      "required": true
    },
    "pageType": {
      "type": "enumeration",
      "enum": [
        "store",
        "event",
        "serviceProvider",
        "messageInBottle",
        "course",
        "workshop",
        "restaurantMenu",
        "generic"
      ],
      "default": "generic",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "isActive": {
      "type": "boolean",
      "default": false
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "city": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "products": {
      "type": "json",
      "default": []
    },
    "metadata": {
      "type": "json",
      "default": {}
    },
    "includeGallery": {
      "type": "boolean",
      "default": false
    },
    "includeFAQ": {
      "type": "boolean",
      "default": false
    },
    "includeTestimonials": {
      "type": "boolean",
      "default": false
    },
    "includeAbout": {
      "type": "boolean",
      "default": false
    },
    "userId": {
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user.user",
      "inversedBy": "pages"
    },
    "purchases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::purchase.purchase",
      "mappedBy": "page"
    },
    "leads": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lead.lead",
      "mappedBy": "page"
    },
    "analytics": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::analytic.analytic",
      "mappedBy": "page"
    },
    "sections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::section.section",
      "mappedBy": "page"
    },
    "storeProducts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "page"
    },
    "guests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::guest.guest",
      "mappedBy": "page"
    },
    "expectedGuests": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "faq": {
      "type": "json",
      "default": []
    },
    "gallery": {
      "type": "json",
      "default": []
    },
    "testimonials": {
      "type": "json",
      "default": []
    },
    "aboutText": {
      "type": "text"
    },
    "services": {
      "type": "json",
      "default": []
    },
    "headerImage": {
      "type": "string"
    },
    "logo": {
      "type": "string"
    },
    "socialMedia": {
      "type": "json",
      "default": {}
    },
    "eventDate": {
      "type": "date"
    },
    "eventTime": {
      "type": "string"
    },
    "eventLocation": {
      "type": "string"
    },
    "workingHours": {
      "type": "json",
      "default": {}
    },
    "subscriptionStatus": {
      "type": "enumeration",
      "enum": ["active", "inactive", "expired"],
      "default": "active"
    },
    "subscriptionExpiry": {
      "type": "datetime"
    }
  }
}
