{
  "kind": "collectionType",
  "collectionName": "analytics",
  "info": {
    "singularName": "analytic",
    "pluralName": "analytics",
    "displayName": "Analytics",
    "description": "Analytics data and events for pages"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "page": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page.page",
      "inversedBy": "analytics"
    },
    "eventType": {
      "type": "enumeration",
      "enum": [
        "view",
        "lead",
        "sale"
      ]
    },
    "eventData": {
      "type": "json"
    },
    "timestamp": {
      "type": "datetime"
    },
    "source": {
      "type": "string"
    },
    "amount": {
      "type": "decimal"
    },
    "totalSales": {
      "type": "decimal",
      "default": 0
    },
    "totalOrders": {
      "type": "integer",
      "default": 0
    },
    "totalCustomers": {
      "type": "integer",
      "default": 0
    },
    "totalLeads": {
      "type": "integer",
      "default": 0
    },
    "dailySales": {
      "type": "json",
      "default": {}
    },
    "monthlySales": {
      "type": "json",
      "default": {}
    },
    "topProducts": {
      "type": "json",
      "default": []
    },
    "recentPurchases": {
      "type": "json",
      "default": []
    }
  }
}
